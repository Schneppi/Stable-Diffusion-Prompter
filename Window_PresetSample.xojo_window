#tag DesktopWindow
Begin DesktopWindow Window_PresetSample
   Backdrop        =   0
   BackgroundColor =   &cFFFFFF
   Composite       =   False
   DefaultLocation =   2
   FullScreen      =   False
   HasBackgroundColor=   False
   HasCloseButton  =   False
   HasFullScreenButton=   False
   HasMaximizeButton=   False
   HasMinimizeButton=   False
   Height          =   600
   ImplicitInstance=   True
   MacProcID       =   0
   MaximumHeight   =   32000
   MaximumWidth    =   32000
   MenuBar         =   ""
   MenuBarVisible  =   False
   MinimumHeight   =   64
   MinimumWidth    =   64
   Resizeable      =   False
   Title           =   "Preset Sample"
   Type            =   4
   Visible         =   True
   Width           =   600
End
#tag EndDesktopWindow

#tag WindowCode
	#tag Event
		Sub Deactivated()
		  Self.Close
		End Sub
	#tag EndEvent

	#tag Event
		Function KeyDown(key As String) As Boolean
		  Self.Close
		  
		  Return True
		End Function
	#tag EndEvent

	#tag Event
		Function MouseDown(x As Integer, y As Integer) As Boolean
		  Self.Close
		  
		  Return True
		End Function
	#tag EndEvent

	#tag Event
		Sub Opening()
		  Self.Left = Screen(0).Left + 100
		  Self.Top = Screen(0).Top + 100
		  Self.Width = Screen(0).Width - 200
		  Self.Height = Screen(0).Height - 200
		End Sub
	#tag EndEvent

	#tag Event
		Sub Paint(g As Graphics, areas() As Rect)
		  If PresetSample=Nil Then Self.Close
		  
		  Var p As Picture
		  p = Scale_Proportional(PresetSample,g.Width,g.Height)
		  
		  If g.Width>g.Height Then
		    
		    g.DrawPicture(p,(g.Width/2)-(p.Width/2),0)
		    
		  Else
		    
		    g.DrawPicture(p,0,(g.Height/2)-(p.Height/2))
		    
		  End If
		End Sub
	#tag EndEvent


	#tag Property, Flags = &h0
		PresetSample As Picture
	#tag EndProperty


#tag EndWindowCode

